<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-2.0.dtd">
<hibernate-configuration>
 <session-factory name="">
  <property name="connection.datasource">java:comp/env/jdbc/MD_CD</property>
  <property name="show_sql">false</property>
  <!--  property name="dialect">net.sf.hibernate.dialect.PostgreSQLDialect</property -->
  <property name="dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
  <mapping resource="it/bncf/magazziniDigitali/database/entity/MDFilesTmp.hbm.xml"/>
  <mapping resource="it/bncf/magazziniDigitali/database/entity/MDFilesTmpError.hbm.xml"/>
  <mapping resource="it/bncf/magazziniDigitali/database/entity/MDRegistroIngresso.hbm.xml"/>
  <mapping resource="it/bncf/magazziniDigitali/database/entity/MDRegistroIngressoError.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
